                <% stars.to_i.times do %>
                  <span class="fa fa-star" style="color: #ffb400"></span>
                <% end %>
                <% blank_stars = 5 - stars.to_i %>
                <% blank_stars.to_i.times do %>
                  <span class="fa fa-star" style="color: #cacccd"></span>
                <% end %>
